<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingHorizontal="@dimen/common_margin_53">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/h80"
        android:includeFontPadding="false"
        android:paddingBottom="@dimen/h10"
        android:text="@string/store"
        android:textColor="@color/color_white"
        android:textSize="@dimen/font_30"
        android:textStyle="bold" />

    <com.biiiiit.kokonats.ui.custome.SponsorType1View
        android:id="@+id/sponsor_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/h40" />

</LinearLayout>